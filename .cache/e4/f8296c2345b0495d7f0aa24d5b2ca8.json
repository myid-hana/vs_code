n    bindAddBookButton();\n\n    // 토큰 체크\n    const token = getToken();\n    if (token === null) {\n        location.assign('/login.html');\n        return;\n    }\n}\n\ndocument.addEventListener('DOMContentLoaded', main);"},"lineCount":null}},"error":null,"hash":"88a6de968d03311eadc6d515dbe36dd2","cacheData":{"env":{}}}